'use strict';























global.setImmediate=require('setImmediate');

require('promise/setimmediate/done');
if(__DEV__){
require('promise/setimmediate/rejection-tracking').enable({
allRejections:true,
onUnhandled:function(id,error){var 
message=error.message;var stack=error.stack;
var warning=
'Possible Unhandled Promise Rejection (id: '+id+'):\n'+(
message==null?'':message+'\n')+(
stack==null?'':stack);
console.warn(warning);},

onHandled:function(id){
var warning=
'Promise Rejection Handled (id: '+id+')\n'+
'This means you can ignore any previous messages of the form '+('"Possible Unhandled Promise Rejection (id: '+
id+'):"');
console.warn(warning);}});}







require('promise/setimmediate/es6-extensions').prototype.finally=function(onSettled){
return this.then(onSettled,onSettled);};



module.exports=require('promise/setimmediate/es6-extensions');